general {
        output_format = "i3bar"
        colors = true
        interval = 5
}

order += "disk /"
order += "ethernet eth0"
order += "tztime local"
order += "Stocks"
  

ethernet eth0 {
        # if you use %speed, i3status requires the cap_net_admin capability
        format_up = "E: %ip (%speed)"
        format_down = "E: down"
}

battery 0 {
        format = "%status %percentage %remaining %emptytime"
        path = "/sys/class/power_supply/BAT%d/uevent"
        low_threshold = 10
}

tztime local {
        format = "%Y-%m-%d %H:%M:%S"
}

Stocks {
    hide_on_error = False
    Tickers = "CPH:TPSL,STO:SDRLO,CPH:LUXOR-B"
}

disk "/" {
        format = "%free"
}
